<%= bootstrap_form_for(@worksession) do |f| %>
	<%= f.text_field :date, :class => 'datepicker', :style => "width: 150px;"%>

	<%= f.text_field :begin_at, class: 'timepicker', :style => "width: 100px", inline: true %>

	<%= f.text_field :end_at, class: 'timepicker', :style => "width: 100px", inline: true %>
	<%= f.text_area :notes, as: :text %>
	<%= f.button :Submit, class: 'btn btn-default'  %>
<% end %>

